root = true

[*]
indent_style = tab
indent_size = 4
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
max_line_length = 88

[*.tsx]
indent_style = space
indent_size = 4
quote_type = double
insert_final_newline = true
trim_trailing_whitespace = true
semicolon = true



